{
  "env": {
    "es6": true,
    "node": true
  },
  "files": ["lib/**/*.js", "test/**/*.{mjs,js}"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-prototype-builtins": 0,
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  }
}
