{
  "name": "makes",
  "version": "3.0.3",
  "description": "A tool to scaffold new projects, simple enough that you would want to define your own skeletons (aka generators).",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "makes": "bin/makes.js"
  },
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava",
    "lint": "eslint lib test",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "build": "ncc build lib/index.js -m",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/makesjs/makes.git"
  },
  "keywords": [
    "scaffold",
    "app",
    "boilerplate"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makesjs/makes/issues"
  },
  "homepage": "https://github.com/makesjs/makes#readme",
  "devDependencies": {
    "@babel/code-frame": "^7.16.7",
    "@vercel/ncc": "^0.34.0",
    "ansi-colors": "^4.1.3",
    "ava": "^3.15.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.17.0",
    "gunzip-maybe": "^1.4.2",
    "hosted-git-info": "^5.0.0",
    "https-proxy-agent": "^5.0.1",
    "isutf8": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.mergewith": "^4.6.2",
    "mock-fs": "^5.1.2",
    "mri": "^1.2.0",
    "nyc": "^15.1.0",
    "sisteransi": "^1.0.5",
    "standard-changelog": "^2.0.27",
    "tar-fs": "^2.1.1",
    "tmp": "^0.2.1",
    "vinyl": "^2.2.1"
  },
  "ava": {
    "concurrency": 1,
    "files": [
      "test/**/*.spec.js"
    ]
  }
}
